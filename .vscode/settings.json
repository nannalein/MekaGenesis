{
    "json.schemas": [
        {
            "fileMatch": [
                "/lang/*.json"
            ],
            "url": "./.vscode/probe.lang-schema.json"
        },
        {
            "fileMatch": [
                "/probe/docs/*.json"
            ],
            "url": "./.vscode/probe.doc-schema.json"
        }
    ],
    "files.exclude": {
        "**/saves/": true,
        "**/patchouli_books/": true,
        "**/nbtexporter/": true,
        "**/ldlib/": true,
        "**/coremods/": true,
        "**/schematics/": true,
        "**/screenshots/": true,
        "**/server-resource-packs/": true,
        "**/shaderpacks/": true,
        "**/texturepacks/": true,
        "**/XaeroWaypoints_BACKUP240807/": true,
        "**/XaeroWaypoints_BACKUP032021/": true,
        "**/xaero/": true,
        "**/logs/": true,
        "**/crash-reports/": true,
        "**/usernamecache.json": true,
        "**/usercache.json": true,
        "**/servers.dat": true,
        "**/servers.dat_old": true,
        "**/hotbar.nbt": true,
        "**/icon.png": true,
        "**/knownkeys.txt": true,
        "**/options.txt": true,
        "**/patchouli_data.json": true,
        "**/rhino.local.properties": true,
        "**/.qmenu_opened.marker": true,
        "**/fabricloader.log": true
    }
}